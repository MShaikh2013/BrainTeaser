<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="10dp" />
            <stroke android:width="1dp" android:color="#C1CD48"/>
            <gradient android:angle="-90" android:startColor="#929C3A" android:endColor="#E4F357" />
        </shape>
    </item>

    <item android:state_focused="true">
        <shape android:shape="rectangle" >
            <corners android:radius="10dp" />
            <stroke android:width="1dp" android:color="#CDDC43" />
            <solid android:color="#E4F357"/>
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle" >
            <corners android:radius="10dp" />
            <stroke android:width="1dp" android:color="#BECC42" />
            <gradient android:angle="-90" android:startColor="#C0CD42" android:endColor="#E4F357" />
        </shape>
    </item>
</selector>